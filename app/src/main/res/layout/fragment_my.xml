<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_my"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_180"
        android:background="#333333"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:text="我的"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_20"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iv_head"
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_80"
            android:layout_marginLeft="@dimen/sp_20"
            android:layout_marginBottom="@dimen/dp_30"
            android:src="@mipmap/u208"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/tv_denglu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/sp_20"
            android:text="点击登录"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_20"
            app:layout_constraintBottom_toBottomOf="@id/iv_head"
            app:layout_constraintLeft_toRightOf="@id/iv_head"
            app:layout_constraintTop_toTopOf="@id/iv_head" />

        <ImageView
            android:id="@+id/iv_jinru"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_marginRight="@dimen/sp_20"
            android:src="@mipmap/jinru"
            app:layout_constraintBottom_toBottomOf="@id/tv_denglu"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/tv_denglu" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cl_my">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_dingdan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/dingdan" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的订单"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_youhuiquan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/youhuiquan" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="优  惠  券"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_lipinka"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/lipinka" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="礼  品  卡"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:background="#bbbbbb"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_shoucang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/shoucang" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的收藏"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_zuji"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/zuji" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的足迹"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_huiyuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/fuli" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="会员福利"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:background="#bbbbbb"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_dizhi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/dizhi" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地址管理"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_zhanghao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/zhanghao" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="账号安全"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_kefu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/kefu" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系客服"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:background="#bbbbbb"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_bangzhu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/bangzhu" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="帮助中心"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />

            <LinearLayout
                android:id="@+id/ll_yijian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="@dimen/dp_29"
                android:paddingRight="@dimen/dp_29"
                android:paddingBottom="@dimen/dp_15"
                android:paddingTop="@dimen/dp_15"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@mipmap/yijian" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="意见反馈"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_15" />
            </LinearLayout>
            <View
                android:layout_width="@dimen/dp_1"
                android:layout_height="match_parent"
                android:background="#bbbbbb" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>