<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.xm2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="UnusedAttribute">
        <activity android:name=".ui.home.activity.ZhiZaoActivity"></activity>
        <activity android:name=".ui.home.activity.adapter.NewActivity"></activity>
        <activity android:name=".ui.my.activity.AddDiZhiActivity" />
        <activity android:name=".ui.my.activity.DiZhiActivity" />
        <activity android:name=".ui.my.activity.ZhuCeActivity" />
        <activity android:name=".ui.my.activity.LoginActivity" />
        <activity android:name=".ui.home.activity.DetailGoodActivity" />
        <activity android:name=".base.ImageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>